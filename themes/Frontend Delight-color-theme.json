	"semanticHighlighting": false,
	"tokenColors": [
		{
		  "name": "Comment",
		  "scope": [
			"comment",
			"string.comment.buffered.block.jade"
		  ],
		  "settings": {
			"fontStyle": "italic",
			"foreground": "#70B880"
		  }
		},
		{
		  "name": "Class",
		  "scope": "entity.name.type.instance",
		  "settings": {
			"fontStyle": "bold",
			"foreground": "#ff8b19"
		  }
		},
		{
		  "name": "Function",
		  "scope": "entity.name.function",
		  "settings": {
			"fontStyle": "bold",
			"foreground": "#ff8b19"
		  }
		},
		{
		  "name": "Support",
		  "scope": "support.class.js",
		  "settings": {
			"fontStyle": "italic",
			"foreground": "#f6485d"
		  }
		},
		{
		  "name": "Storage",
		  "scope": "storage",
		  "settings": {
			"fontStyle": "italic",
			"foreground": "#ffd2a690"
		  }
		},
		{
		  "name": "Keyword",
		  "scope": "keyword",
		  "settings": {
			"fontStyle": "italic",
			"foreground": "#ff681e"
		  }
		},
		{
		  "name": "String",
		  "scope": [
			"string.quoted.double.css",
